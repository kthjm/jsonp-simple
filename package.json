{
  "name": "jsonp-simple",
  "version": "0.0.0",
  "private": true,
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "kthjm/jsonp-simple",
  "license": "MIT",
  "main": "lib",
  "description": "",
  "keywords": [],
  "scripts": {
    "pretest": "flow",
    "test": "cross-env NODE_ENV=test nyc mocha test.js",
    "report": "nyc report --reporter=lcov",
    "codecov": "codecov",
    "coverage": "run-s report codecov",
    "prebuild": "yarn test",
    "build": "babel index.js -d ./lib",
    "postbuild": "prettier --write **/*.js package.json"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.0.5",
    "flow-bin": "^0.59.0",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "rewire": "^3.0.1",
    "sinon": "^4.0.1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          }
        }
      ],
      "flow"
    ],
    "env": {
      "test": {
        "presets": ["power-assert"],
        "plugins": ["istanbul"]
      }
    }
  },
  "nyc": {
    "require": ["babel-register", "jsdom-global/register"],
    "sourceMap": false,
    "instrument": false
  }
}
